using System;
using System.Collections.Generic;

namespace Stacks_and_Queues
{
    class Program
    {
        static void Main(string[] args)
        {
            string text = Console.ReadLine();
            Stack<char> stack = new Stack<char>(text);

            while (stack.Count > 0)
            {
                Console.Write(stack.Pop());
            }
        }
    }
}
